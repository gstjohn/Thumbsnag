{
    "name": "gstjohn/thumbsnag",
    "description": "Thumbsnag crawls an HTML document and finds imagery that best represents the given page.",
    "keywords": ["image", "crawler", "thumbnail", "opengraph"],
    "license": "MIT",
    "authors": [
        {
            "name": "Garrett St. John",
            "email": "gstjohn@gmail.com"
        }
    ],
    "require": {
        "fastimage/fastimage": "dev-master#c9b452aa315a65d1129bb9a820e18ee7130af066",
        "webignition/absolute-url-deriver": "~1.5"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0@dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pinleague/fastimage.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Thumbsnag\\": "src/"
        }
    }
}
